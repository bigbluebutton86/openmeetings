      <xs:attribute name="onshadowangle" type="xs:string" >
<xs:annotation>
<xs:documentation xml:lang="en">
Sent when shadowangle changes </xs:documentation>
</xs:annotation>
</xs:attribute>
      <xs:attribute name="onremovesubview" type="xs:string" >
