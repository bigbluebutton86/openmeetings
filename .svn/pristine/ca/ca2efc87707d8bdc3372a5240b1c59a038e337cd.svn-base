<xs:element name="drawview" type="lzx:drawview" >
<xs:annotation>
<xs:documentation xml:lang="en">


    @copyright Copyright 2001-2010 Laszlo Systems, Inc.  All Rights Reserved.
               Use is subject to license terms.

    @access public
    @affects lzdrawview
    @topic LFC
    @subtopic Views
  </xs:documentation>
</xs:annotation>
