          &lt;code&gt;
          &lt;dataset name="dsHours"&gt;
              &lt;item value="1"&gt;1&lt;/item&gt;
              &lt;item value="2"&gt;2&lt;/item&gt;
              &lt;item value="3"&gt;3&lt;/item&gt;
