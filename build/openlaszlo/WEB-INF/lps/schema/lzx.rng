      <interleave>
        <ref name="viewContent"/>
        <attribute name="href">
          
          <data type="anyURI"/>
        </attribute>
        <attribute name="stage">
          <choice>
            <value>late</value>
            <value>defer</value>
          </choice>
        </attribute>
        <optional>
          <attribute name="onload">
            <ref name="script"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="proxied" a:defaultValue="inherit">
            <choice>
              <ref name="booleanLiteral"/>
              <value>inherit</value>
            </choice>
          </attribute>
        </optional>
        <optional>
          <attribute name="onerror">
            <ref name="script"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="ontimeout">
            <ref name="script"/>
          </attribute>
        </optional>
      </interleave>
    </element>
  </define>
  <define name="state">
    <element name="state">
      <interleave>
        <ref name="viewContent"/>
        <optional>
          <attribute name="apply">
            <ref name="boolean"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="pooling" a:since="1.1" a:defaultValue="false">
            
            <ref name="boolean"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="onapply">
            
            <ref name="script"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="onremove">
            
            <ref name="script"/>
          </attribute>
        </optional>
      </interleave>
    </element>
  </define>
  <define name="nodeAttributes">
    <ref name="objectAttributes"/>
    <optional>
      <attribute name="defaultplacement">
        
        <data type="string" datatypeLibrary=""/>
      </attribute>
    </optional>
  </define>
  <define name="nodeElements">
    <interleave>
      <ref name="objectElements"/>
      <ref name="datapathChildren"/>
      <zeroOrMore>
        <element name="data">
          <interleave>
            <ref name="datasetAttributes"/>
            <ref name="anyXML"/>
          </interleave>
        </element>
      </zeroOrMore>
      <zeroOrMore>
        <element name="dataset">
          <interleave>
            <ref name="datasetAttributes"/>
            <ref name="anyXML"/>
          </interleave>
        </element>
      </zeroOrMore>
    </interleave>
  </define>
  <define name="nodeContent">
    <interleave>
      <ref name="nodeAttributes"/>
      <ref name="nodeElements"/>
    </interleave>
  </define>
  <define name="node">
    <element name="node">
      <ref name="nodeContent"/>
    </element>
  </define>
  <define name="datarequest">
    <element name="datarequest">
      <ref name="nodeContent"/>
    </element>
  </define>
  <define name="layout">
    <element name="layout">
      <interleave>
        <ref name="nodeContent"/>
        <optional>
          <attribute name="locked">
            <ref name="booleanLiteral"/>
          </attribute>
        </optional>
      </interleave>
    </element>
  </define>
  
  <define name="splash">
    <element name="splash">
      <interleave>
        <optional>
          <attribute name="hideafterinit">
            <ref name="booleanLiteral"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="persistent" a:defaultValue="false">
            <ref name="booleanLiteral"/>
          </attribute>
        </optional>
        <zeroOrMore>
          <element name="view">
            <interleave>
              <optional>
                <attribute name="name">
                  <data type="token" datatypeLibrary=""/>
                </attribute>
              </optional>
              <attribute name="resource">
                <data type="anyURI"/>
              </attribute>
              <optional>
                <attribute name="ratio">
                  <data type="string"/>
                </attribute>
              </optional>
              <optional>
                <attribute name="x" a:defaultValue="0">
                  <ref name="numberLiteral"/>
                </attribute>
              </optional>
              <optional>
                <attribute name="y" a:defaultValue="0">
                  <ref name="numberLiteral"/>
                </attribute>
              </optional>
              <optional>
                <attribute name="center" a:defaultValue="false">
                  <ref name="booleanLiteral"/>
                </attribute>
              </optional>
            </interleave>
          </element>
        </zeroOrMore>
      </interleave>
    </element>
  </define>
  <define name="scriptElement">
    <element name="script">
      <interleave>
        <optional>
          <attribute name="type" a:defaultValue="Javascript">
            
            <data type="string"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="when" a:defaultValue="normal">
            <choice>
              <value>normal</value>
              <value>immediate</value>
            </choice>
          </attribute>
        </optional>
        <choice>
          <attribute name="src">
            <data type="anyURI"/>
          </attribute>
          <optional>
            <text/>
          </optional>
        </choice>
      </interleave>
    </element>
  </define>
  <define name="styleSheetElement">
    <element name="stylesheet">
      <choice>
        <attribute name="src">
          
          <data type="anyURI"/>
        </attribute>
        <optional>
          <text/>
        </optional>
      </choice>
    </element>
  </define>
  <define name="preloadresource">
    <element name="preloadresource">
      <interleave>
        <optional>
          <attribute name="name">
            <data type="token" datatypeLibrary=""/>
          </attribute>
        </optional>
        <optional>
          <attribute name="ratio">
            <data type="string" datatypeLibrary=""/>
          </attribute>
        </optional>
        <attribute name="synctoload">
          <ref name="booleanLiteral"/>
        </attribute>
        <attribute name="hideafterinit">
          <ref name="booleanLiteral"/>
        </attribute>
        <attribute name="center">
          <ref name="booleanLiteral"/>
        </attribute>
        <optional>
          <attribute name="synchronized">
            <ref name="booleanLiteral"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="lastframe">
            <ref name="numberLiteral"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="resource">
            <data type="string" datatypeLibrary=""/>
          </attribute>
        </optional>
        <optional>
          <attribute name="resourcename">
            <data type="string" datatypeLibrary=""/>
          </attribute>
        </optional>
        <ref name="viewAttributes"/>
      </interleave>
    </element>
  </define>
  
  <define name="pattern">
    <element name="pattern">
      <text/>
    </element>
  </define>
  <define name="allow">
    <element name="allow">
      <zeroOrMore>
        <ref name="pattern"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="deny">
    <element name="deny">
      <zeroOrMore>
        <ref name="pattern"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="security">
    <element name="security">
      <interleave>
        <optional>
          <ref name="allow"/>
        </optional>
        <optional>
          <ref name="deny"/>
        </optional>
      </interleave>
    </element>
  </define>
</grammar>